<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".fragments.RegisterFragment">
    <androidx.core.widget.NestedScrollView
            android:layout_width="0dp"
            android:layout_height="match_parent" android:fillViewport="true"
            app:layout_constraintEnd_toStartOf="@+id/confirmReButton"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="1.0">
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp" android:layout_marginStart="10dp" android:layout_marginBottom="10dp"
                android:layout_marginEnd="20dp">
            <TextView
                    android:text="Dane osobowe"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView" android:layout_marginBottom="30dp"/>
            <TextView
                    android:text="Imie"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView2"/>
            <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:id="@+id/nameLabelReg" android:layout_marginBottom="10dp"
                    tools:ignore="TouchTargetSizeCheck"
                    android:hint="Imie"/>
            <TextView
                    android:text="Nazwisko"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView3"/>
            <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:id="@+id/secondNameLabelReg" android:layout_marginBottom="10dp"
                    tools:ignore="TouchTargetSizeCheck" android:hint="Nazwisko"/>
            <TextView
                    android:text="email"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView4"/>
            <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:id="@+id/editTextTextPersonNameReg" android:layout_marginBottom="10dp"
                    tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" android:hint="email"/>
            <TextView
                    android:text="login"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView8"/>
            <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:id="@+id/loginRe" tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck"
                    android:hint="login" android:layout_marginBottom="10dp"/>
            <TextView
                    android:text="hasło"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView12"/>
            <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:id="@+id/passReg" tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck"
                    android:hint="hasło" android:layout_marginBottom="10dp"/>
            <TextView
                    android:text="numer telefonu"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView5"/>
            <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="phone"
                    android:ems="10"
                    android:id="@+id/phoneLabelReg" android:layout_marginBottom="10dp"
                    tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" android:hint="numer telefonu"/>
            <TextView
                    android:text="miasto"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView6"/>
            <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:id="@+id/cityLabelReg"
                    tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" android:hint="miasto"
                    android:layout_marginBottom="10dp"/>
            <TextView
                    android:text="ulica"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView7"/>
            <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:id="@+id/streetLabelReg" tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck"
                    android:hint="ulica" android:layout_marginBottom="10dp"/>
            <TextView
                    android:text="numer domu/mieszkania"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView9"/>
            <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:id="@+id/numberBuildReg" tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck"
                    android:hint="numer domu" android:layout_marginBottom="10dp"/>
            <TextView
                    android:text="kod pocztowy"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView10"/>
            <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:id="@+id/codeLabelReg" tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck"
                    android:hint="kod pocztowy"/>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
    <Button
            android:text="Confirm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/confirmReButton"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="16dp"
            app:layout_constraintVertical_bias="0.014"/>
</androidx.constraintlayout.widget.ConstraintLayout>